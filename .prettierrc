{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"insertPragma": false,
	"jsxBracketSameLine": true,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
